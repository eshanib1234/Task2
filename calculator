x=int(input("Enter a num: "))
y=int(input("Enter a num: "))
op=input('Enter the operatio, (+,-,*,//) to perform: ')
def add(x,y):
    return x+y
def sub(x,y):
    return x-y
def mul(x,y):
    return x*y
def div(x,y):
    return x//y
    
if op=='+':
    result=add(x,y)
elif op=='-':
    result=sub(x,y)
elif op=='*':
    result=mul(x,y)
elif op=='//':
    result=div(x,y)
else:
    print('Enter a valid operator')
    
print(f"Result: {result}")
